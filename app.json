{
	"expo": {
		"name": "icodes-mobapp",
		"slug": "icodes-mobapp",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/logo.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash-icodes.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.khylaehza.icodesmobapp"
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/logo.png",
				"backgroundColor": "#ffffff"
			},
			"permissions": ["INTERNET"],
			"package": "com.khylaehza.icodesmobapp"
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "e838cda6-781d-4f75-9b38-6777c81933a5"
			}
		}
	}
}
